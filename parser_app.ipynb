    "import pandas as pd\n",
    "import json\n",
    "import os\n",
    "from notification_reader import process_notification_data\n",
    "\n",
    "def debug_config_files():\n",
    "    \"\"\"Debug function to check if config files exist and are valid\"\"\"\n",
    "    print(\"=== CONFIG FILE DEBUG ===\")\n",
    "    \n",
    "    # Check dictionary.json\n",
    "    dict_file = 'dictionary.json'\n",
    "    if os.path.exists(dict_file):\n",
    "        print(f\"✓ {dict_file} exists\")\n",
    "        try:\n",
    "            with open(dict_file, 'r', encoding='utf-8') as f:\n",
    "                dict_data = json.load(f)\n",
    "            print(f\"✓ {dict_file} is valid JSON\")\n",
    "            \n",
    "            # Check required keys\n",
    "            required_keys = ['categories', 'merchants', 'transaction_types', 'blacklist']\n",
    "            for key in required_keys:\n",
    "                if key in dict_data:\n",
    "                    print(f\"✓ Found '{key}' in dictionary\")\n",
    "                    if key == 'transaction_types':\n",
    "                        print(f\"  - transaction_types has keys: {list(dict_data[key].keys())}\")\n",
    "                else:\n",
    "                    print(f\"✗ Missing '{key}' in dictionary\")\n",
    "                    \n",
    "        except json.JSONDecodeError as e:\n",
    "            print(f\"✗ {dict_file} has JSON error: {e}\")\n",
    "        except Exception as e:\n",
    "            print(f\"✗ Error reading {dict_file}: {e}\")\n",
    "    else:\n",
    "        print(f\"✗ {dict_file} not found\")\n",
    "    \n",
    "    # Check regex_patterns.json\n",
    "    patterns_file = 'regex_patterns.json'\n",
    "    if os.path.exists(patterns_file):\n",
    "        print(f\"✓ {patterns_file} exists\")\n",
    "        try:\n",
    "            with open(patterns_file, 'r', encoding='utf-8') as f:\n",
    "                patterns_data = json.load(f)\n",
    "            print(f\"✓ {patterns_file} is valid JSON\")\n",
    "            \n",
    "            # Check required keys\n",
    "            required_keys = ['amount_patterns', 'account_patterns']\n",
    "            for key in required_keys:\n",
    "                if key in patterns_data:\n",
    "                    print(f\"✓ Found '{key}' in patterns\")\n",
    "                else:\n",
    "                    print(f\"✗ Missing '{key}' in patterns\")\n",
    "                    \n",
    "        except json.JSONDecodeError as e:\n",
    "            print(f\"✗ {patterns_file} has JSON error: {e}\")\n",
    "        except Exception as e:\n",
    "            print(f\"✗ Error reading {patterns_file}: {e}\")\n",
    "    else:\n",
    "        print(f\"✗ {patterns_file} not found\")\n",
    "    \n",
    "    print(\"=== END CONFIG DEBUG ===\\n\")\n",
    "\n",
    "def main():\n",
    "    # Debug config files first\n",
    "    debug_config_files()\n",
    "    \n",
    "    # Define file paths\n",
    "    notifications_file = 'raw-data-1.csv'\n",
    "    dictionary_file = 'dictionary.json'\n",
    "    patterns_file = 'regex_patterns.json'\n",
    "    \n",
    "    output_file = 'processed_transactions_1.csv'\n",
    "    \n",
    "    try:\n",
    "        # Load the dataset\n",
    "        print(\"Loading notifications data...\")\n",
    "        notifications_df = pd.read_csv(notifications_file, quotechar='\"', escapechar='\\\\')\n",
    "        print(f\"Loaded {len(notifications_df)} notifications\")\n",
    "        \n",
    "        # Filter out unwanted system notifications (keep only financial apps)\n",
    "        financial_apps = []\n",
    "        filtered_df = notifications_df[~notifications_df['app_label'].isin(financial_apps)]\n",
    "        \n",
    "        # Update column mapping for new CSV structure\n",
    "        column_mapping = {\n",
    "            '_id': 'id',\n",
    "            'package_name': 'package_name',\n",
    "            'app_label': 'app_label', \n",
    "            'title': 'message',  # Map 'title' to 'message'\n",
    "            'text': 'contents',  # Map 'text' to 'contents'\n",
    "            'post_time': 'timestamp'  # Map 'post_time' to 'timestamp'\n",
    "        }\n",
    "        \n",
    "        # Rename columns to match what the parser expects\n",
    "        filtered_df = filtered_df.rename(columns=column_mapping)\n",
    "        \n",
    "        # Add user_id column if it doesn't exist (using a default value)\n",
    "        if 'user_id' not in filtered_df.columns:\n",
    "            filtered_df['user_id'] = 'default_user'\n",
    "        \n",
    "        # Save filtered data for inspection\n",
    "        filtered_df.to_csv('custom_notifications_1.csv', index=False)\n",
    "        print(f\"Filtered to {len(filtered_df)} notifications after keeping only financial apps\")\n",
    "        \n",
    "        # Show sample data for debugging\n",
    "        if len(filtered_df) > 0:\n",
    "            print(\"\\nSample notifications:\")\n",
    "            for i, row in filtered_df.head(3).iterrows():\n",
    "                print(f\"App: {row['app_label']}\")\n",
    "                print(f\"Title: {row['message']}\")\n",
    "                print(f\"Text: {row['contents']}\")\n",
    "                print(\"---\")\n",
    "        else:\n",
    "            print(\"No financial app notifications found!\")\n",
    "            print(\"Available apps in dataset:\")\n",
    "            print(notifications_df['app_label'].value_counts().head(10))\n",
    "            return\n",
    "        \n",
    "        # Show app distribution\n",
    "        app_counts = filtered_df['app_label'].value_counts()\n",
    "        print(\"\\nCount of financial notifications by app label:\")\n",
    "        print(app_counts)\n",
    "        \n",
    "        # Process the data with dictionary and patterns files\n",
    "        print(\"\\nProcessing notifications...\")\n",
    "        \n",
    "        # Try to process with more detailed error handling\n",
    "        try:\n",
    "            results_df = process_notification_data(\n",
